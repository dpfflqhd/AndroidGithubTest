<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ThirdFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/child_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F1F0F0">

        <Button
            android:id="@+id/btn_jeju"
            android:layout_width="49dp"
            android:layout_height="34dp"
            android:layout_marginStart="110dp"
            android:layout_marginEnd="252dp"
            android:background="@drawable/jeju"
            android:text="제주"
            android:textColor="#636363"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_gwang" />

        <Button
            android:id="@+id/btn_geungsang"
            android:layout_width="119dp"
            android:layout_height="177dp"
            android:layout_marginEnd="84dp"
            android:layout_marginBottom="60dp"
            android:background="@drawable/gyeongsang"
            android:text="대구·부산\n울산·경상"
            android:textColor="#636363"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/GoogleMapView1"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/btn_gwang"
            android:layout_width="113dp"
            android:layout_height="128dp"
            android:layout_marginStart="110dp"
            android:layout_marginTop="73dp"
            android:background="@drawable/jeonra"
            android:text="광주·전라"
            android:textColor="#636363"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_chung" />

        <Button
            android:id="@+id/btn_chung"
            android:layout_width="151dp"
            android:layout_height="81dp"
            android:layout_marginStart="110dp"
            android:layout_marginTop="93dp"
            android:background="@drawable/choong"
            android:text="대전·충청"
            android:textColor="#636363"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <Button
            android:id="@+id/btn_seoul"
            android:layout_width="97dp"
            android:layout_height="87dp"
            android:layout_marginStart="125dp"
            android:layout_marginTop="23dp"
            android:background="@drawable/gyeonggi"
            android:text="서울·인천\n경기"
            android:textColor="#6C6C6C"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <Button
            android:id="@+id/btn_gangwon"
            android:layout_width="123dp"
            android:layout_height="91dp"
            android:layout_marginTop="7dp"
            android:layout_marginEnd="100dp"
            android:background="@drawable/gangwon"
            android:text="강원"
            android:textColor="#636363"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView6" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="62dp"
            android:layout_marginTop="24dp"
            android:background="#FFFFFF"
            android:src="@drawable/logocolor2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:srcCompat="@drawable/simple_line" />

        <com.google.android.gms.maps.MapView
            android:id="@+id/GoogleMapView1"
            android:name="com.google.android.gms.maps.MapFragment"
            android:layout_width="332dp"
            android:layout_height="204dp"
            android:layout_marginBottom="112dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>