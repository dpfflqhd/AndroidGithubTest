<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment3Child1">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/child_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="0dp"
            android:layout_height="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3"
            app:srcCompat="@drawable/simple_line" />

        <RelativeLayout
            android:id="@+id/MapView"
            android:layout_width="300dp"
            android:layout_height="300dp"
            android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.414"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.215">

        </RelativeLayout>

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="62dp"
            android:layout_marginTop="24dp"
            android:src="@drawable/logocolor2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/btn_jeonra"
            android:layout_width="115dp"
            android:layout_height="142dp"
            android:layout_marginStart="195dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="2dp"
            android:background="@drawable/jeonra"
            app:layout_constraintEnd_toStartOf="@+id/btn_choong"
            app:layout_constraintHorizontal_bias="0.45"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_choong" />

        <Button
            android:id="@+id/btn_gangwon"
            android:layout_width="129dp"
            android:layout_height="93dp"
            android:layout_marginTop="268dp"
            android:layout_marginEnd="104dp"
            android:background="@drawable/gangwon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/btn_gyeonggi"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" />

        <Button
            android:id="@+id/btn_choong"
            android:layout_width="153dp"
            android:layout_height="83dp"
            android:layout_marginStart="162dp"
            android:layout_marginEnd="3dp"
            android:background="@drawable/choong"
            app:layout_constraintEnd_toStartOf="@+id/btn_gyeongsang"
            app:layout_constraintHorizontal_bias="0.517"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btn_gangwon" />

        <Button
            android:id="@+id/btn_gyeongsang"
            android:layout_width="121dp"
            android:layout_height="179dp"
            android:layout_marginTop="87dp"
            android:layout_marginEnd="88dp"
            android:layout_marginBottom="14dp"
            android:background="@drawable/gyeongsang"
            app:layout_constraintBottom_toTopOf="@+id/btn_jeonra"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/btn_gangwon"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/btn_gyeonggi"
            android:layout_width="99dp"
            android:layout_height="89dp"
            android:layout_marginStart="114dp"
            android:background="@drawable/gyeonggi"
            app:layout_constraintBottom_toTopOf="@+id/btn_choong"
            app:layout_constraintStart_toStartOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>

